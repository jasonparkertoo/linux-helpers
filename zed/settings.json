 {
  "base_keymap": "JetBrains",
  "ui_font_size": 20,
  "buffer_font_size": 20,
  "theme": {
    "mode": "system",
    "light": "One Dark",
    "dark": "JetBrains New Dark"
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.gitignore",
    "**/.gitattributes",
    "**/.classpath",
    "**/.settings",
    "**/.factorypath",
    "**/.project"
    // "**/target"
  ],
  "languages": {
    "Go": {
      "show_inline_completions": true
    },
    "Java": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "google-java-format",
          "arguments": ["-a", "-"]
        }
      }
    }
  },
  "auto_update_extensions": {
    "java": true
  },
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  }
}
